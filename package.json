{
    "name": "@anglr/select",
    "version": "0.0.0-placeholder",
    "description": "Angular component representing html select",
    "main": "dist/es5/index.js",
    "main-es2015": "dist/es2015/index.js",
    "typings": "dist/es5/index.d.ts",
    "sideEffects": false,
    "scripts": {
        "build": "npm run ngc && npm run ngc -- -p tsconfig.es2015.json",
        "postbuild": "npm run postbuild:copy && npm run postbuild:clean",
        "postbuild:copy": "copyfiles -u 3 dist/es5/src/**/*.* dist/es5 && copyfiles -u 3 dist/es2015/src/**/*.* dist/es2015 && copyfiles -u 2 dist/testing/**/*.* testing",
        "postbuild:clean": "rimraf dist/es5/src dist/es5/dist dist/es5/node_modules dist/es5/testing dist/es5/karma-* dist/es2015/src dist/es2015/dist dist/es2015/node_modules dist/es2015/testing dist/es2015/karma-*",
        "clean": "rimraf dist testing tests docs",
        "clean:deps": "rimraf package-lock.json node_modules",
        "clean:all": "npm run clean && npm run clean:deps",
        "ngc": "ngc",
        "version:placeholder": "replace-in-file /\\\"version\\\":\\s\\\".*?\\\"/g \"\\\"version\\\": \\\"0.0.0-placeholder\\\"\" package.json --isRegex",
        "version:git": "ngv -e \"replace-in-file 0.0.0-placeholder $GIT_VERSION package.json\"",
        "prepublishOnly": "npm run bundle && npm run version:placeholder && npm run version:git",
        "postpublish": "npm run version:placeholder && npm run clean",
        "bundle": "npm run clean && npm run build && nnrh *.js -d dist/es5 --htmlDir src && nnrh *.js -d dist/es2015 --htmlDir src",
        "deploy": "npm publish",
        "deploy:local": "npm publish --registry http://local:4873 && npm run docs",
        "deploy:next": "npm publish --tag next",
        "deploy:next:local": "npm publish --tag next --registry http://local:4873 && npm run docs",
        "docs": "compodoc src -d docs -n '@anglr/select' -p tsconfig.json --disablePrivate --disableInternal --disableLifeCycleHooks",
        "postdocs": "copyfiles -u 1 docs/**/*.* \\\\DOCS-SERVER\\docs\\ng-select && npm run clean",
        "pretest": "webpack",
        "test": "karma start karma.conf.js"
    },
    "repository": {
        "type": "git",
        "url": "git+https://gitlab.com/Buko/ng-select.git"
    },
    "keywords": [
        "angular",
        "ng",
        "select"
    ],
    "author": "kukjevov",
    "license": "MIT",
    "bugs": {
        "url": "https://gitlab.com/Buko/ng-select/issues"
    },
    "homepage": "https://gitlab.com/Buko/ng-select#README",
    "peerDependencies": {
        "@angular/core": ">=7.0.0",
        "@angular/common": ">=7.0.0",
        "@angular/forms": ">=7.0.0",
        "rxjs": ">=6.3.0",
        "@jscrpt/common": ">=1.0.0",
        "positions": "^1.6.2"
    },
    "devDependencies": {
        "@angular/core": "^7.0.0",
        "@angular/common": "^7.0.0",
        "@angular/forms": "^7.0.0",
        "rxjs": "^6.3.0",
        "@jscrpt/common": "^1.0.0",
        "@types/jasmine": "2.8.11",
        "@anglr/types": "1.1.15",
        "@angular/compiler-cli": "^7.0.0",
        "@angular/compiler": "^7.0.0",
        "@angular/language-service": "^7.0.0",
        "@angular/platform-browser-dynamic": "^7.0.0",
        "@angular/platform-browser": "^7.0.0",
        "phantomjs-prebuilt": "2.1.16",
        "karma-cli": "1.0.1",
        "karma": "3.1.1",
        "karma-jasmine": "1.1.2",
        "karma-phantomjs-launcher": "1.0.4",
        "karma-chrome-launcher": "2.2.0",
        "karma-webpack": "3.0.5",
        "karma-jasmine-html-reporter": "1.4.0",
        "jasmine": "3.3.0",
        "awesome-typescript-loader": "5.2.1",
        "angular2-template-loader": "0.6.2",
        "raw-loader": "0.5.1",
        "sass-loader": "7.1.0",
        "css-loader": "1.0.1",
        "file-loader": "2.0.0",
        "style-loader": "0.23.1",
        "@compodoc/compodoc": "1.1.6",
        "rimraf": "2.6.2",
        "copyfiles": "2.1.0",
        "replace-in-file": "3.4.2",
        "typescript": "3.1.6",
        "webpack": "4.25.1",
        "npm-git-version": "1.1.0",
        "npm-ng-require-html": "1.0.2"
    }
}

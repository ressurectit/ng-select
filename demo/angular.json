{
    "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
    "version": 1,
    "projects":
    {
        "app":
        {
            "root": "",
            "sourceRoot": "",
            "projectType": "application",
            "architect":
            {
                "build":
                {
                    "builder": "@angular-devkit/build-angular:application",
                    "options":
                    {
                        "baseHref": "/",
                        "outputPath":
                        {
                            "base": "wwwroot",
                            "browser": "browser",
                            "server": "server"
                        },
                        "index": "index.html",
                        "browser": "app/main.browser.ts",
                        "tsConfig": "tsconfig.app.json",
                        "inlineStyleLanguage": "scss",
                        "styles":
                        [
                            "content/site.scss",
                            "content/light.scss",
                            "content/dark.scss"
                        ],
                        "allowedCommonJsDependencies":
                        [
                            "mousetrap",
                            "extend",
                            "sourcemapped-stacktrace",
                            "store",
                            "dayjs",
                            "@braintree/sanitize-url",
                            "dompurify",
                            "elkjs",
                            "cytoscape-cose-bilkent"
                        ],
                        "scripts":
                        [
                            {
                                "input": "config/configBrowserOverride.js",
                                "bundleName": "configBrowserOverride"
                            }
                        ],
                        "serviceWorker": false,
                        "security": 
                        {
                            "autoCsp": false
                        }
                    },
                    "configurations":
                    {
                        "production":
                        {
                            "define":
                            {
                                "isProduction": "true",
                                "jsDevMode": "false"
                            },
                            "budgets":
                            [
                                {
                                    "type": "initial",
                                    "maximumWarning": "2mb",
                                    "maximumError": "4mb"
                                },
                                {
                                    "type": "anyComponentStyle",
                                    "maximumWarning": "2kb",
                                    "maximumError": "4kb"
                                }
                            ],
                            "outputHashing": "all"
                        },
                        "development":
                        {
                            "define":
                            {
                                "isProduction": "false",
                                "jsDevMode": "true"
                            },
                            "optimization": false,
                            "extractLicenses": false,
                            "sourceMap": true
                        }
                    },
                    "defaultConfiguration": "development"
                },
                "serve":
                {
                    "builder": "@angular-devkit/build-angular:dev-server",
                    "options":
                    {
                        "hmr": true,
                        "proxyConfig": "proxy.conf.json",
                        "port": 8888
                    },
                    "configurations":
                    {
                        "production":
                        {
                            "buildTarget": "app:build:production"
                        },
                        "development":
                        {
                            "buildTarget": "app:build:development"
                        }
                    },
                    "defaultConfiguration": "development"
                }
            }
        }
    },
    "cli":
    {
        "cache":
        {
            "environment": "local",
            "enabled": false
        },
        "analytics": false,
        "warnings":
        {
            "versionMismatch": true
        }
    }
}